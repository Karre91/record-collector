{
	// See: https://record-collector.net/en/updates/continuous-deployment-using-just-netlify/
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	},
	"AppSettings": {
		"SiteUrl": "https://record-collector.netlify.app/",
		"ContentRootPath": "/opt/build/repo/docs/content-record-collector-net/en/",
		"StaticSiteRootPath": "/opt/build/repo/artifacts/static-site/",
		"FrontendSetup": "default",
		"ViewPrefix": "",
		"SectionsToExcludeFromLists": [
			"pages",
			"sidor"
		],
		"MainNavigationSections": [
			"pages",
			"sidor"
		],
		"PaginationPageCount": 2, // Hur många sidor du kan hoppa till direkt
		"PaginationPageSize": 3 //Hur många uppdates som visas per sida
	}
}
